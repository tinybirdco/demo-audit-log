# Data Source created from Pipe 'mat_join'

SCHEMA >
    `datetime` DateTime64(3),
    `company_id` Int8,
    `name` LowCardinality(String),
    `event` String,
    `device_OS` String,
    `device_browser` String,
    `size` UInt32,
    `plan` LowCardinality(String)

ENGINE "MergeTree"
ENGINE_PARTITION_KEY "toYYYYMM(datetime)"
ENGINE_SORTING_KEY "datetime, intHash32(size)"
